    "import xport, csv\n",
    "with xport.XportReader('in.xpt') as reader:\n",
    "    with open('out.csv', 'rb') as out:\n",
    "        writer = csv.DictWriter(out, [f['name'] for f in reader.fields])\n",
    "        for row in reader:\n",
    "            writer.writerow(row)"
